# Docker Environment Variables
# Copy to .env and fill with your actual values

# Database
DB_PASSWORD=your-secure-password-here

# Session
SESSION_SECRET=generate-with-crypto-randomBytes-64-hex

# Hyperliquid API
HYPERLIQUID_API_KEY=your-hyperliquid-api-key
HYPERLIQUID_PRIVATE_KEY=your-hyperliquid-private-key
HYPERLIQUID_TESTNET=true

# OpenRouter AI
OPENROUTER_API_KEY=your-openrouter-api-key
AI_MODEL=moonshot/kimi-k2

# Frontend API URLs (for Docker)
VITE_API_URL=http://localhost:4000
VITE_WS_URL=ws://localhost:4000

# Environment
NODE_ENV=production
